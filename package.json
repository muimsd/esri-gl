{
  "name": "esri-map-gl",
  "version": "0.0.7",
  "type": "module",
  "description": "A module for making it easier to use Esri services in mapbox-gl or maplibre-gl.",
  "main": "dist/esri-map-gl.js",
  "module": "dist/esri-map-gl.esm.js",
  "browser": "dist/esri-map-gl.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run type-check && rollup -c",
    "build:prod": "npm run clean && npm run type-check && npm run lint && rollup -c",
    "build:watch": "rollup -c --watch",
    "build:example": "vite build --outDir dist-demo",
    "preview:example": "vite preview --outDir dist-demo",
    "build-docs": "cross-env NODE_ENV=production webpack --config docs/webpack.config.js --mode production --progress --hide-modules",
    "dev:docs": "cross-env webpack-dev-server --config docs/webpack.config.js --mode development --open --hot",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 50",
    "lint:fix": "eslint src --ext ts,tsx,js,jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test:ci",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "prepublishOnly": "npm run validate && npm run build:prod",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.5",
    "@types/maplibre-gl": "^1.13.2",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^5.0.2",
    "ant-design-vue": "^1.7.3",
    "ava": "^1.0.1",
    "benchmark": "^2.1.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^8.49.0",
    "eslint-config-mourner": "^3.0.0",
    "esm": "^3.0.84",
    "file-loader": "^6.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-loader": "^0.5.7",
    "load-json-file": "^5.1.0",
    "mapbox-gl": "^2.15.0",
    "maplibre-gl": "^3.6.2",
    "prettier": "^3.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "remarkable": "^2.0.1",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "rollup-plugin-typescript2": "^0.35.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontiersi/mapbox-gl-esri-sources"
  },
  "homepage": "https://frontiersi.github.io/mapbox-gl-esri-sources/",
  "author": "Rowan Winsemius",
  "license": "Apache-2.0"
}
